# Path to your oh-my-zsh installation.
export ZSH=$HOME/dotfiles/zsh

source $ZSH/antigen/antigen.zsh

antigen use oh-my-zsh

# User configuration
# Bundles from the default repo (robbyrussell's oh-my-zsh).
antigen bundle git
antigen bundle heroku
antigen bundle pip
antigen bundle lein
antigen bundle command-not-found

# Syntax highlighting bundle.
antigen bundle zsh-users/zsh-syntax-highlighting

# Load the theme.
antigen theme dracula

antigen apply

export PATH="/home/arkokoley/bin:/home/arkokoley/.composer/vendor/bin:/home/arkokoley/mongodb/mongodb-linux-x86_64-2.6.3/bin:/home/arkokoley/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/home/arkokoley/.rvm/bin:/home/arkokoley/.rvm/bin:/home/arkokoley/go/bin:/home/arkokoley/.rvm/bin"

### Added by the Heroku Toolbelt
export PATH="/usr/local/heroku/bin:$PATH"
